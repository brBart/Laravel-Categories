{
    "name": "faustbrian/laravel-categorizable",
    "description": "Categorizable Polymorphic Eloquent Models for Laravel 5",
    "keywords": ["laravel", "framework", "Laravel-Categorizable", "Laravel Categorizable", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": ">=7.0.0",
        "illuminate/database": "5.4.* || 5.5.*",
        "illuminate/support": "5.4.* || 5.5.*",
        "kalnoy/nestedset": "^4.3",
        "spatie/laravel-sluggable": "^2.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Categorizable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Categorizable\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BrianFaust\\Categorizable\\CategorizableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
